{
  "name": "flatten-to-key-value",
  "version": "1.0.1",
  "description": "Flatten any JSON-like value into key/value pairs with dot paths and joined arrays.",
  "license": "MIT",
  "author": "Prabath",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prabath95/flatten-to-key-value.git"
  },
  "bugs": {
    "url": "https://github.com/prabath95/flatten-to-key-value/issues"
  },
  "homepage": "https://github.com/prabath95/flatten-to-key-value#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "flatten",
    "json",
    "key-value",
    "dotpath",
    "typescript",
    "utility"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --sourcemap --watch",
    "test": "vitest run",
    "lint": "eslint .",
    "prepublishOnly": "npm run build && npm test && npm run lint"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.5.4",
    "vitest": "^2.0.0"
  }
}
